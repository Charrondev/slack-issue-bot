{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "object-curly-spacing": [2, "never"],
    "max-len": [2, {"code": 140, "tabWidth": 2, "ignoreUrls": true}],
    "no-console": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-throw-literal": 0
  }
}
